        "jasmine-spec-reporter": "4.1.1",
        "json-server": "0.12.0",
        "jsonwebtoken": "7.4.1",
        "karma": "1.7.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-cli": "1.0.1",
        "karma-jasmine": "1.1.0",
        "karma-remap-istanbul": "0.6.0",
